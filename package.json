{
  "name": "angular2-instagram-core",
  "version": "0.0.4-2",
  "description": "Angular2 Instagram Core serivces",
  "main": "release/index.js",
  "typings": "release/index.d.ts",
  "scripts": {
    "test": "karma start",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "clean": "npm-run-all -p clean:*",
    "clean:build": "rimraf build",
    "clean:release": "rimraf release",
    "copy:build": "cpx \"src/**/*\" build",
    "build": "webpack --display-error-details",
    "build:package": "cross-env NODE_ENV=package npm run build",
    "package:aot": "ngc -p tsconfig-aot.json",
    "package:minify": "uglifyjs release/index.js --source-map release/index.min.js.map --source-map-url release/index.js.map --compress --mangle --screw-ie8 --output release/index.min.js",
    "package": "npm-run-all -s clean copy:build package:aot build:package package:minify clean:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JayKan/angular2-instagram-core.git"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "Angular4",
    "Angular Services"
  ],
  "authors": [
    "Jay Kan"
  ],
  "license": "MIT",
